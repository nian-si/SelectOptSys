T = 16000;
K = 16;
L = log2(K);
type = 2; % 1 for newsvendor 2 for queueing
cur_K = 16;
val_K = zeros(K,2);
val_K(:,2) = 1:cur_K;
for level = 1:L
    level
    T_l = floor(T/cur_K/L);
    for j = 1:cur_K
        [val_K(j,1),opt_sol,avg_n_customer,avg_D,avg_W] = run_system(val_K(j,2),type,T_l,K);
        [val_K(j,2),opt_sol,avg_n_customer,avg_D,avg_W]
    end
    val_K = -sortrows(-val_K);
    cur_K = cur_K/2;
end
if (val_K(1,2) == 6)
    correct = correct + 1;
